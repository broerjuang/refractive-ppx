  },
  "devDependencies": {
    "@esy-ocaml/reason": "^3.5.0",
    "@opam/dune": "^1.11.0",
    "@opam/ppx_tools_versioned": "^5.2.3",
    "refmterr": "*",
    "ocaml": "~4.2.3"
  }
}